#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pub/Desktop/CodeSpace/Personal/themeforest-NVY3MIfb-tokyo-tailwind-css-personal-portfolio-react-next-js-template/tokyo/node_modules/.pnpm/tailwindcss@3.3.5/node_modules/tailwindcss/lib/node_modules:/Users/pub/Desktop/CodeSpace/Personal/themeforest-NVY3MIfb-tokyo-tailwind-css-personal-portfolio-react-next-js-template/tokyo/node_modules/.pnpm/tailwindcss@3.3.5/node_modules/tailwindcss/node_modules:/Users/pub/Desktop/CodeSpace/Personal/themeforest-NVY3MIfb-tokyo-tailwind-css-personal-portfolio-react-next-js-template/tokyo/node_modules/.pnpm/tailwindcss@3.3.5/node_modules:/Users/pub/Desktop/CodeSpace/Personal/themeforest-NVY3MIfb-tokyo-tailwind-css-personal-portfolio-react-next-js-template/tokyo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pub/Desktop/CodeSpace/Personal/themeforest-NVY3MIfb-tokyo-tailwind-css-personal-portfolio-react-next-js-template/tokyo/node_modules/.pnpm/tailwindcss@3.3.5/node_modules/tailwindcss/lib/node_modules:/Users/pub/Desktop/CodeSpace/Personal/themeforest-NVY3MIfb-tokyo-tailwind-css-personal-portfolio-react-next-js-template/tokyo/node_modules/.pnpm/tailwindcss@3.3.5/node_modules/tailwindcss/node_modules:/Users/pub/Desktop/CodeSpace/Personal/themeforest-NVY3MIfb-tokyo-tailwind-css-personal-portfolio-react-next-js-template/tokyo/node_modules/.pnpm/tailwindcss@3.3.5/node_modules:/Users/pub/Desktop/CodeSpace/Personal/themeforest-NVY3MIfb-tokyo-tailwind-css-personal-portfolio-react-next-js-template/tokyo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../tailwindcss/lib/cli.js" "$@"
fi
